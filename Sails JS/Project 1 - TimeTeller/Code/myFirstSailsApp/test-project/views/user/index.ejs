<div class="container">
  <h2>Users</h2>
  <p>User table for the TimeTeller API</p>
  <table class="table table-striped">
    <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    </thead>
    <tbody>
    <% _.each(user, function(user){ %>
    <tr data-id="<%user.id%>" datamodel="user">
      <td><%= user.id%></td>
      <td><%= user.name%></td>
      <td><%= user.email%></td>
      <td><a href="/user/show/<%=user.id%>">Show</a></td>
      <td><a href="/user/edit/<%=user.id%>">Edit</a></td>
      <td><a href="/user/destroy/<%=user.id%>">Delete</a></td>
    </tr>

    <%})%>
    </tbody>
  </table>
</div>
